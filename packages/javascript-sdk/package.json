{
  "name": "@usermaven/sdk-js",
  "version": "0.0.0",
  "description": "Usermaven JavaScript SDK.",
  "main": "dist/npm/usermaven.cjs.js",
  "module": "dist/npm/usermaven.es.js",
  "types": "dist/npm/usermaven.d.ts",
  "files": [
    "dist/npm/*",
    "dist/web/*"
  ],
  "author": "Usermaven <hello@usermaven.com>",
  "scripts": {
    "clean": "rm -rf ./dist",
    "devserver": "PORT=8081 nodemon --watch '__tests__/common/*.ts' --exec 'ts-node' __tests__/common/devserver.ts",
    "test": "rollup -c && jest --detectOpenHandles --verbose false",
    "build": "rollup -c"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/*.spec.{ts,js}"
    ],
    "automock": false,
    "verbose": false
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.22",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "fetch-cookie": "^1.0.0",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-playwright-preset": "^1.7.0",
    "next": "^12.0.8",
    "node-fetch": "^3.1.0",
    "nodemon": "^2.0.15",
     "playwright": "^1.19.2",
    "rollup": "^2.70.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^27.1.2",
    "ts-node": "9.1.1",
    "tslib": "^2.3.1",
    "typescript": "4.4.3"
  },
  "resolutions": {
   
  }
}
